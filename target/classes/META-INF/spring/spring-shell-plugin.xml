<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xmlns:tx="http://www.springframework.org/schema/tx"
	xsi:schemaLocation="
	http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans-3.1.xsd
	http://www.springframework.org/schema/context
	http://www.springframework.org/schema/context/spring-context-3.1.xsd">
	<context:component-scan base-package="com.example.HILLOGY_library_exam" />
	<bean id="InitLibrary" class="com.example.HILLOGY_library_exam.InitLibrary"></bean>
	<bean id="InitUsers" class="com.example.HILLOGY_library_exam.InitUsers"></bean>
	<bean id="BookCommads"
		class="com.example.HILLOGY_library_exam.commands.BookCommands"></bean>
</beans>
